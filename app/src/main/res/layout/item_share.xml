<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="wrap_content"
                                                   android:background="@drawable/background_item_share">

    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textViewEmail"
            app:layout_constraintStart_toEndOf="@+id/imageView2"
            app:layout_constraintTop_toBottomOf="@+id/textViewName" android:layout_marginTop="4dp"
            android:layout_marginStart="28dp"/>
    <ImageView
            android:layout_width="41dp"
            android:layout_height="43dp"
            app:srcCompat="@drawable/ic_avatar_user"
            android:id="@+id/imageView2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="20dp"
            android:layout_marginStart="24dp"
            android:contentDescription="TODO"
            tools:ignore="VectorDrawableCompat" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="20dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textViewName"
            app:layout_constraintStart_toEndOf="@+id/imageView2" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="20dp"
            android:layout_marginStart="28dp"/>
    <ImageView
            android:layout_width="33dp"
            android:layout_height="29dp" app:srcCompat="@drawable/ic_cancel"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/imageShareCancel"
            app:layout_constraintVertical_bias="0.487" android:layout_marginEnd="16dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>